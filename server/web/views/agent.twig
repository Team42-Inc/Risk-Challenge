<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
    google.charts.load('current', {'packages':['corechart']});
    google.charts.setOnLoadCallback(draw{{ char_name }});

    function draw{{ char_name }}() {
        var data = google.visualization.arrayToDataTable([
            [{{ char_x_name|raw }}, {{ char_y_names|raw }}],
            {{ char_datas|raw }}
        ]);

        {% if char_options  %}
        var options = {{ char_options }};
        {% else %}
        var options =  {
            title: 'title',
            curveType: 'function',
            legend: { position: 'bottom' }
        };
        {% endif %}


        var chart = new google.visualization.LineChart(document.getElementById('chart_{{ char_name }}'));

        chart.draw(data, options);
    }
</script>
<div id="chart_{{ char_name }}" style="width: 900px; height: 500px"/>