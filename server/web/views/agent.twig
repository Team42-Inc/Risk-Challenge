<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
    google.charts.load('current', {'packages':['corechart']});
    google.charts.setOnLoadCallback(draw{{ char_name }});

    function draw{{ char_name }}() {
        var data = google.visualization.arrayToDataTable([
            [{{ char_x_name }}, {{ char_y_names }}],
            {{ char_datas }}
        ]);

        var options = {{ char_option }};/* {
            title: 'title',
            curveType: 'function',
            legend: { position: 'bottom' }
        };
        */
        var chart = new google.visualization.LineChart(document.getElementById('chart_{{ char_name }}'));

        chart.draw(data, options);
    }
</script>
<div id="char_{{ char_name }}" style="width: 900px; height: 500px"/>