{% extends "wrapper.twig" %}

{% block sidebarmenu %}
    <div class="user-panel">
        <div class="pull-left image">
            <img src="./assets/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
        </div>
        <div class="pull-left info">
            <p>{{ username|default('Dark Vador') }}</p>
            <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
        </div>
    </div>
{% endblock %}

{% block content %}

<div> {{ username }} has been add successfully, please send this QRCode to your staff </div>
<div>
    <img src="{{ urlQRCode|raw }}" alt="{{ secret|raw }}" /><br/>
    {{ secret|raw }}
</div>

{% endblock %}
